# Auxeira Ferrari Dashboard Environment Configuration

# Application
VITE_APP_NAME=Auxeira Ferrari Dashboards
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# API Configuration
VITE_API_BASE_URL=https://api.auxeira.com
VITE_API_VERSION=v1

# Payment Integration (Paystack)
VITE_PAYSTACK_PUBLIC_KEY=pk_test_your_paystack_public_key_here
PAYSTACK_SECRET_KEY=sk_test_your_paystack_secret_key_here

# AI Integration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_THUNDERBIT_API_KEY=your_thunderbit_api_key_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/auxeira_db

# Authentication
VITE_AUTH_DOMAIN=auxeira.auth0.com
VITE_AUTH_CLIENT_ID=your_auth0_client_id
VITE_AUTH_AUDIENCE=https://api.auxeira.com

# Feature Flags
VITE_ENABLE_AI_MENTOR=true
VITE_ENABLE_VOICE_INTERFACE=true
VITE_ENABLE_BLOCKCHAIN_VERIFICATION=false
VITE_ENABLE_QUANTUM_SECURITY=false
VITE_ENABLE_REAL_TIME_ANALYTICS=true
VITE_ENABLE_PREMIUM_REPORTS=true
VITE_ENABLE_API_ACCESS=true

# Dashboard Configuration
VITE_DEFAULT_DASHBOARD=startup_founder
VITE_ENABLE_SUBSCRIPTION_GATING=true
VITE_TRIAL_PERIOD_DAYS=30

# External Integrations
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_key
VITE_SALESFORCE_CLIENT_ID=your_salesforce_client_id
VITE_QUICKBOOKS_CLIENT_ID=your_quickbooks_client_id

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Monitoring
VITE_SENTRY_DSN=your_sentry_dsn
VITE_HOTJAR_ID=your_hotjar_id

# CDN & Assets
VITE_CDN_URL=https://cdn.auxeira.com
VITE_ASSETS_URL=https://assets.auxeira.com

# Social & Marketing
VITE_TWITTER_HANDLE=@auxeira
VITE_LINKEDIN_URL=https://linkedin.com/company/auxeira
VITE_DISCORD_INVITE=https://discord.gg/auxeira

# Development
VITE_DEBUG_MODE=true
VITE_MOCK_DATA=true
VITE_ENABLE_DEVTOOLS=true
