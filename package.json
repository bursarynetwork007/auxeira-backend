    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "typedoc": "^0.25.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
