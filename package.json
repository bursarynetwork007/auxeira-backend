{
  "name": "auxeira-sse-backend",
  "version": "1.0.0",
  "description": "Auxeira Startup Success Engine Backend - Scalable SaaS Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "ts-node src/database/migrate.ts",
    "db:seed": "ts-node src/database/seed.ts",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "docker:build": "docker build -t auxeira-backend .",
    "docker:run": "docker run -p 3000:3000 auxeira-backend"
  },
  "keywords": [
    "startup",
    "sse",
    "behavioral-tracking",
    "ai-mentor",
    "saas",
    "typescript",
    "express"
  ],
  "author": "Auxeira Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "ioredis": "^5.3.2",
    "socket.io": "^4.7.2",
    "axios": "^1.5.0",
    "openai": "^4.0.0",
    "stripe": "^13.0.0",
    "nodemailer": "^6.9.4",
    "winston": "^3.10.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1450.0",
    "kafkajs": "^2.2.4",
    "cron": "^2.4.4",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.196",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/nodemailer": "^6.4.9",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bursarynetwork007/auxeira-backend.git"
  },
  "bugs": {
    "url": "https://github.com/bursarynetwork007/auxeira-backend/issues"
  },
  "homepage": "https://auxeira.com"
}
