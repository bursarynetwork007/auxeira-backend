{
  "name": "auxeira-backend",
  "version": "2.0.0",
  "description": "Auxeira SSE Platform - Complete Backend API for Social, Sustainability, and Economic scoring platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",

    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate": "ts-node src/database/migrate.ts",
    "seed": "ts-node src/database/seed.ts",
    "db:reset": "npm run migrate && npm run seed",
    "docker:build": "docker build -t auxeira-backend .",
    "docker:run": "docker run -p 3000:3000 auxeira-backend"
  },
  "keywords": [
    "sse",
    "sustainability",
    "social-impact",
    "economic-scoring",
    "ai-mentorship",
    "startup-platform",
    "typescript",
    "nodejs",
    "postgresql",
    "websocket",
    "real-time"
  ],
  "author": "Auxeira Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "nodemailer": "^6.9.7",
    "stripe": "^14.5.0",
    "socket.io": "^4.7.4",
    "openai": "^4.20.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "axios": "^1.6.2",
    "cron": "^3.1.6",
    "csv-parser": "^3.0.0",
    "pdf-lib": "^1.17.1",
    "handlebars": "^4.7.8",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "express-async-errors": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^6.4.14",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
